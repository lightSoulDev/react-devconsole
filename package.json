{
  "name": "react-devconsole",
  "version": "1.0.0",
  "description": "A drop-in development logger with a built-in console UI for debuging React applications.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dev-console",
    "logger",
    "development",
    "debug",
    "console",
    "react",
    "typescript"
  ],
  "author": "Nurzhan Kozhanov: @lightSoulDev",
  "license": "Apache License 2.0",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./styles": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lightSoulDev/react-devconsole"
  },
  "bugs": {
    "url": "https://github.com/lightSoulDev/react-devconsole/issues"
  },
  "homepage": ""
}